#include <iostream>
namespace first{ // the name of the namespace can also be anything bit we can not use digits.
    int x = 0;
}
 
// we can not use digits at the first posn in the identifier of the namespace.
namespace a2{
    char y = '$';
}


int main() {
    using namespace a2;
    using namespace std;

    cout << first:: x;

    
    return 0;
}